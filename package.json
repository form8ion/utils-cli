{
  "name": "@form8ion/utils-cli",
  "description": "cli for various tools for the organization",
  "license": "MIT",
  "version": "0.0.0-semantically-released",
  "bin": {
    "form8ion-utils": "bin/form8ion-utils.js"
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=12.20"
  },
  "files": [
    "bin/"
  ],
  "repository": "form8ion/utils-cli",
  "bugs": "https://github.com/form8ion/utils-cli/issues",
  "homepage": "https://npm.im/@form8ion/utils-cli",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "scripts": {
    "test": "npm-run-all --print-label --parallel lint:* --parallel test:*",
    "test:unit": "cross-env NODE_ENV=test c8 run-s test:unit:base",
    "test:unit:base": "DEBUG=any mocha 'src/**/*-test.js'",
    "coverage:report": "c8 report --reporter=text-lcov > coverage.lcov && codecov",
    "test:integration": "run-s 'test:integration:base -- --profile noWip'",
    "test:integration:base": "NODE_OPTIONS=--enable-source-maps DEBUG=any cucumber-js test/integration --profile base",
    "test:integration:debug": "DEBUG=nock.* run-s test:integration",
    "test:integration:wip": "run-s 'test:integration:base -- --profile wip'",
    "test:integration:focus": "run-s 'test:integration:base -- --profile focus'",
    "lint:js": "eslint . --cache",
    "lint:md": "remark . --frail",
    "lint:sensitive": "ban",
    "lint:engines": "ls-engines",
    "lint:gherkin": "gherkin-lint",
    "lint:peer": "npm ls >/dev/null",
    "clean": "rimraf ./bin",
    "prebuild": "run-s clean",
    "build": "npm-run-all --print-label --parallel build:*",
    "build:js": "rollup --config",
    "watch": "run-s 'build:js -- --watch'",
    "prepack": "run-s build",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@babel/node": "7.16.8",
    "@babel/register": "7.17.0",
    "@cucumber/cucumber": "7.3.2",
    "@form8ion/babel-preset": "1.6.83",
    "@form8ion/commitlint-config": "1.0.29",
    "@form8ion/core": "1.5.0",
    "@form8ion/eslint-config": "2.0.3",
    "@form8ion/eslint-config-cucumber": "1.4.0",
    "@form8ion/eslint-config-mocha": "1.2.17",
    "@form8ion/remark-lint-preset": "2.1.7",
    "@rollup/plugin-node-resolve": "13.1.3",
    "@travi/any": "2.0.19",
    "ban-sensitive-files": "1.9.17",
    "c8": "7.11.0",
    "chai": "4.3.6",
    "clear-module": "4.1.2",
    "codecov": "3.8.3",
    "cross-env": "7.0.3",
    "cz-conventional-changelog": "3.3.0",
    "gherkin-lint": "4.2.2",
    "http-status-codes": "2.2.0",
    "husky": "7.0.4",
    "import-fresh": "3.3.0",
    "js-yaml": "4.1.0",
    "ls-engines": "0.6.5",
    "mdast-util-from-markdown": "0.8.5",
    "mdast-zone": "4.0.1",
    "mocha": "9.2.2",
    "mock-fs": "5.1.2",
    "nock": "13.2.4",
    "npm-run-all": "4.1.5",
    "remark-cli": "10.0.1",
    "rimraf": "3.0.2",
    "rollup": "2.70.0",
    "rollup-plugin-auto-external": "2.0.0",
    "rollup-plugin-executable": "1.6.3",
    "rollup-plugin-json": "4.0.0",
    "sinon": "13.0.1",
    "testdouble": "3.16.4",
    "unist-util-find": "1.0.2"
  },
  "dependencies": {
    "@form8ion/codecov": "2.0.1",
    "@form8ion/cucumber-scaffolder": "1.5.0",
    "@form8ion/github-actions-node-ci": "3.2.0",
    "@form8ion/hapi-scaffolder": "4.0.0",
    "@form8ion/javascript": "3.5.3",
    "@form8ion/javascript-core": "4.0.1",
    "@form8ion/lift": "2.5.0",
    "@form8ion/mocha-scaffolder": "1.2.0",
    "@form8ion/project": "11.1.0",
    "@form8ion/remark-plugin-scaffolder": "2.0.0",
    "@form8ion/remove-greenkeeper": "1.1.8",
    "@form8ion/renovate-scaffolder": "1.7.0",
    "@form8ion/replace-travis-ci-with-github-actions": "3.0.2",
    "@form8ion/scaffolder-scaffolder": "2.1.0",
    "@travi/github-scaffolder": "7.0.0",
    "source-map-support": "0.5.21",
    "travis-token-updater": "1.6.14",
    "update-notifier": "5.1.0",
    "yargs": "17.3.1"
  }
}
