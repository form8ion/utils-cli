{
  "name": "@form8ion/utils-cli",
  "description": "cli for various tools for the organization",
  "license": "MIT",
  "version": "0.0.0-semantically-released",
  "type": "module",
  "engines": {
    "node": "^18.17.0 || >=20.6.1"
  },
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "repository": "form8ion/utils-cli",
  "bugs": "https://github.com/form8ion/utils-cli/issues",
  "homepage": "https://npm.im/@form8ion/utils-cli",
  "bin": {
    "form8ion-utils": "./bin/form8ion-utils.js"
  },
  "sideEffects": false,
  "scripts": {
    "test": "npm-run-all --print-label --parallel lint:* --parallel test:*",
    "test:unit": "cross-env NODE_ENV=test c8 run-s test:unit:base",
    "test:unit:base": "DEBUG=any vitest run",
    "test:integration": "run-s 'test:integration:base -- --profile noWip'",
    "test:integration:base": "NODE_OPTIONS=\"--loader=testdouble --enable-source-maps $NODE_OPTIONS\" DEBUG=any cucumber-js test/integration",
    "test:integration:debug": "DEBUG=nock.* run-s test:integration",
    "test:integration:wip": "run-s 'test:integration:base -- --profile wip'",
    "test:integration:focus": "run-s 'test:integration:base -- --profile focus'",
    "pretest:e2e": "run-s build",
    "test:e2e": "NODE_OPTIONS=--enable-source-maps node ./test/e2e/smoke-test.js",
    "lint:js": "eslint . --cache",
    "lint:md": "remark . --frail",
    "lint:sensitive": "ban",
    "lint:engines": "ls-engines",
    "lint:gherkin": "gherkin-lint",
    "lint:peer": "npm ls >/dev/null",
    "lint:lockfile": "lockfile-lint",
    "clean": "rimraf ./bin",
    "prebuild": "run-s clean",
    "build": "npm-run-all --print-label --parallel build:*",
    "build:js": "rollup --config",
    "watch": "run-s 'build:js -- --watch'",
    "prepack": "run-s build",
    "prepare": "husky install",
    "lint:publish": "publint --strict"
  },
  "files": [
    "bin/"
  ],
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "dependencies": {
    "@form8ion/codecov": "5.0.0",
    "@form8ion/cucumber-scaffolder": "3.1.1",
    "@form8ion/github-actions-node-ci": "4.0.0",
    "@form8ion/hapi-scaffolder": "4.0.7",
    "@form8ion/javascript": "11.10.2",
    "@form8ion/javascript-core": "10.1.0",
    "@form8ion/lift": "9.0.1",
    "@form8ion/mocha-scaffolder": "5.0.0",
    "@form8ion/octoherd-script": "1.0.0",
    "@form8ion/ossf-scorecard": "1.2.0",
    "@form8ion/prettier": "2.0.0",
    "@form8ion/project": "18.0.2",
    "@form8ion/remark-plugin-scaffolder": "3.0.4",
    "@form8ion/remove-greenkeeper": "3.0.1",
    "@form8ion/renovate-scaffolder": "2.1.0",
    "@form8ion/replace-travis-ci-with-github-actions": "10.0.0",
    "@form8ion/rollup": "3.2.0",
    "@form8ion/scaffolder-scaffolder": "6.0.2",
    "@form8ion/vite": "2.0.0",
    "@form8ion/vitest": "4.0.1",
    "@travi/github-scaffolder": "10.0.0",
    "update-notifier": "7.0.0",
    "yargs": "17.7.2"
  },
  "devDependencies": {
    "@cucumber/cucumber": "10.1.0",
    "@form8ion/commitlint-config": "1.0.65",
    "@form8ion/core": "3.2.2",
    "@form8ion/eslint-config": "7.0.8",
    "@form8ion/eslint-config-cucumber": "1.4.1",
    "@form8ion/eslint-config-mocha": "3.0.1",
    "@form8ion/remark-lint-preset": "6.0.2",
    "@rollup/plugin-node-resolve": "15.2.3",
    "@travi/any": "3.0.2",
    "ban-sensitive-files": "1.10.1",
    "c8": "8.0.1",
    "camelcase": "8.0.0",
    "chai": "5.0.0",
    "clear-module": "4.1.2",
    "cross-env": "7.0.3",
    "cz-conventional-changelog": "3.3.0",
    "execa": "8.0.1",
    "gherkin-lint": "4.2.4",
    "http-status-codes": "2.3.0",
    "husky": "8.0.3",
    "import-fresh": "3.3.0",
    "jest-when": "3.6.0",
    "js-yaml": "4.1.0",
    "lockfile-lint": "4.12.1",
    "ls-engines": "0.9.1",
    "mdast-util-from-markdown": "2.0.0",
    "mdast-zone": "6.1.0",
    "mock-fs": "5.2.0",
    "nock": "13.4.0",
    "npm-run-all2": "6.1.1",
    "publint": "0.2.7",
    "remark-cli": "12.0.0",
    "rimraf": "5.0.5",
    "rollup": "4.9.1",
    "rollup-plugin-auto-external": "2.0.0",
    "rollup-plugin-executable": "1.6.3",
    "rollup-plugin-json": "4.0.0",
    "testdouble": "3.20.1",
    "unist-util-find": "3.0.0",
    "vitest": "1.1.0"
  }
}
