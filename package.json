{
  "name": "@form8ion/utils-cli",
  "description": "cli for various tools for the organization",
  "license": "MIT",
  "version": "0.0.0-semantically-released",
  "bin": {
    "form8ion-utils": "bin/form8ion-utils.js"
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=12"
  },
  "files": [
    "bin/"
  ],
  "repository": "form8ion/utils-cli",
  "bugs": "https://github.com/form8ion/utils-cli/issues",
  "homepage": "https://npm.im/@form8ion/utils-cli",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "scripts": {
    "test": "npm-run-all --print-label --parallel lint:* --parallel test:*",
    "test:unit": "nyc run-s test:unit:base",
    "test:unit:base": "DEBUG=any mocha 'src/**/*-test.js'",
    "coverage:report": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "test:integration": "run-s 'test:integration:base -- --profile noWip'",
    "test:integration:base": "NODE_OPTIONS=--enable-source-maps DEBUG=any cucumber-js test/integration --profile base",
    "test:integration:debug": "DEBUG=nock.* run-s test:integration",
    "test:integration:wip": "run-s 'test:integration:base -- --profile wip'",
    "test:integration:focus": "run-s 'test:integration:base -- --profile focus'",
    "lint:js": "eslint . --cache",
    "lint:md": "remark . --frail",
    "lint:sensitive": "ban",
    "lint:engines": "check-engine",
    "lint:gherkin": "gherkin-lint",
    "disabled:lint:peer": "npm ls >/dev/null",
    "clean": "rimraf ./bin",
    "prebuild": "run-s clean",
    "build": "npm-run-all --print-label --parallel build:*",
    "build:js": "rollup --config",
    "watch": "run-s 'build:js -- --watch'",
    "prepack": "run-s build",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@babel/node": "7.16.0",
    "@babel/register": "7.16.0",
    "@cucumber/cucumber": "7.3.1",
    "@form8ion/babel-preset": "1.6.71",
    "@form8ion/commitlint-config": "1.0.22",
    "@form8ion/core": "1.4.2",
    "@form8ion/eslint-config": "1.8.7",
    "@form8ion/eslint-config-cucumber": "1.4.0",
    "@form8ion/eslint-config-mocha": "1.2.12",
    "@form8ion/remark-lint-preset": "2.1.6",
    "@travi/any": "2.0.18",
    "ban-sensitive-files": "1.9.16",
    "chai": "4.3.4",
    "check-engine": "1.10.1",
    "clear-module": "4.1.2",
    "codecov": "3.8.3",
    "cz-conventional-changelog": "3.3.0",
    "gherkin-lint": "4.2.2",
    "http-status-codes": "2.1.4",
    "husky": "7.0.4",
    "import-fresh": "3.3.0",
    "js-yaml": "4.1.0",
    "mocha": "9.1.3",
    "mock-fs": "5.1.2",
    "nock": "13.2.0",
    "npm-run-all": "4.1.5",
    "nyc": "15.1.0",
    "remark-cli": "10.0.0",
    "rimraf": "3.0.2",
    "rollup": "2.59.0",
    "rollup-plugin-auto-external": "2.0.0",
    "rollup-plugin-executable": "1.6.3",
    "rollup-plugin-json": "4.0.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "sinon": "12.0.1",
    "testdouble": "3.16.3"
  },
  "dependencies": {
    "@form8ion/cucumber-scaffolder": "1.5.0",
    "@form8ion/github-actions-node-ci": "1.14.0",
    "@form8ion/hapi-scaffolder": "2.0.6",
    "@form8ion/javascript-core": "4.0.0",
    "@form8ion/lift": "2.1.3",
    "@form8ion/lift-javascript": "3.1.5",
    "@form8ion/mocha-scaffolder": "1.2.0",
    "@form8ion/project": "11.0.1",
    "@form8ion/remark-plugin-scaffolder": "1.2.3",
    "@form8ion/remove-greenkeeper": "1.1.8",
    "@form8ion/renovate-scaffolder": "1.7.0",
    "@form8ion/replace-travis-ci-with-github-actions": "2.0.0",
    "@form8ion/scaffolder-scaffolder": "1.5.6",
    "@travi/github-scaffolder": "7.0.0",
    "@travi/javascript-scaffolder": "14.10.0",
    "source-map-support": "0.5.20",
    "travis-token-updater": "1.6.12",
    "update-notifier": "5.1.0",
    "yargs": "17.2.1"
  }
}
