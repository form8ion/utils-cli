{
  "name": "@form8ion/utils-cli",
  "description": "cli for various tools for the organization",
  "license": "MIT",
  "version": "0.0.0-semantically-released",
  "type": "module",
  "engines": {
    "node": "^16.14.0 || >=18"
  },
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "repository": "form8ion/utils-cli",
  "bugs": "https://github.com/form8ion/utils-cli/issues",
  "homepage": "https://npm.im/@form8ion/utils-cli",
  "bin": {
    "form8ion-utils": "./bin/form8ion-utils.js"
  },
  "sideEffects": false,
  "scripts": {
    "test": "npm-run-all --print-label --parallel lint:* --parallel test:*",
    "test:unit": "cross-env NODE_ENV=test c8 run-s test:unit:base",
    "test:unit:base": "DEBUG=any vitest run",
    "test:integration": "run-s 'test:integration:base -- --profile noWip'",
    "test:integration:base": "NODE_OPTIONS=\"--loader=testdouble --enable-source-maps $NODE_OPTIONS\" DEBUG=any cucumber-js test/integration",
    "test:integration:debug": "DEBUG=nock.* run-s test:integration",
    "test:integration:wip": "run-s 'test:integration:base -- --profile wip'",
    "test:integration:focus": "run-s 'test:integration:base -- --profile focus'",
    "pretest:e2e": "run-s build",
    "test:e2e": "NODE_OPTIONS=--enable-source-maps node ./test/e2e/smoke-test.js",
    "lint:js": "eslint . --cache",
    "lint:md": "remark . --frail",
    "lint:sensitive": "ban",
    "lint:engines": "ls-engines",
    "lint:gherkin": "gherkin-lint",
    "lint:peer": "npm ls >/dev/null",
    "lint:lockfile": "lockfile-lint",
    "clean": "rimraf ./bin",
    "prebuild": "run-s clean",
    "build": "npm-run-all --print-label --parallel build:*",
    "build:js": "rollup --config",
    "watch": "run-s 'build:js -- --watch'",
    "prepack": "run-s build",
    "prepare": "husky install"
  },
  "files": [
    "bin/"
  ],
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "dependencies": {
    "@form8ion/codecov": "4.0.1",
    "@form8ion/cucumber-scaffolder": "2.1.0",
    "@form8ion/github-actions-node-ci": "3.14.2",
    "@form8ion/hapi-scaffolder": "4.0.5",
    "@form8ion/javascript": "11.5.0",
    "@form8ion/javascript-core": "9.0.0",
    "@form8ion/lift": "8.0.0",
    "@form8ion/mocha-scaffolder": "3.0.2",
    "@form8ion/octoherd-script": "1.0.0",
    "@form8ion/ossf-scorecard": "1.2.0",
    "@form8ion/prettier": "1.0.2",
    "@form8ion/project": "16.0.1",
    "@form8ion/remark-plugin-scaffolder": "3.0.2",
    "@form8ion/remove-greenkeeper": "2.0.1",
    "@form8ion/renovate-scaffolder": "1.8.0",
    "@form8ion/replace-travis-ci-with-github-actions": "9.0.1",
    "@form8ion/rollup": "3.0.1",
    "@form8ion/scaffolder-scaffolder": "5.0.1",
    "@form8ion/vite": "1.3.2",
    "@form8ion/vitest": "3.0.2",
    "@travi/github-scaffolder": "9.0.1",
    "update-notifier": "6.0.2",
    "yargs": "17.7.2"
  },
  "devDependencies": {
    "@cucumber/cucumber": "9.2.0",
    "@form8ion/commitlint-config": "1.0.52",
    "@form8ion/core": "2.0.0",
    "@form8ion/eslint-config": "5.0.34",
    "@form8ion/eslint-config-cucumber": "1.4.1",
    "@form8ion/eslint-config-mocha": "2.0.3",
    "@form8ion/remark-lint-preset": "5.0.9",
    "@rollup/plugin-node-resolve": "15.1.0",
    "@travi/any": "2.1.8",
    "ban-sensitive-files": "1.9.19",
    "c8": "8.0.0",
    "chai": "4.3.7",
    "clear-module": "4.1.2",
    "cross-env": "7.0.3",
    "cz-conventional-changelog": "3.3.0",
    "execa": "7.1.1",
    "gherkin-lint": "4.2.2",
    "http-status-codes": "2.2.0",
    "husky": "8.0.3",
    "import-fresh": "3.3.0",
    "jest-when": "3.5.2",
    "js-yaml": "4.1.0",
    "lockfile-lint": "4.10.5",
    "ls-engines": "0.9.0",
    "mdast-util-from-markdown": "1.3.1",
    "mdast-zone": "5.1.1",
    "mock-fs": "5.2.0",
    "nock": "13.3.1",
    "npm-run-all": "4.1.5",
    "remark-cli": "11.0.0",
    "rimraf": "5.0.1",
    "rollup": "3.26.0",
    "rollup-plugin-auto-external": "2.0.0",
    "rollup-plugin-executable": "1.6.3",
    "rollup-plugin-json": "4.0.0",
    "testdouble": "3.18.0",
    "unist-util-find": "2.0.0",
    "vitest": "0.32.2"
  }
}
